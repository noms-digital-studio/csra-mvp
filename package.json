  "description": "Frontend prototype for CSRA",
  "main": "index.js",
  "engines": {
    "node": "^6.x"
  },
  "scripts": {
    "build": "rm -rf dist && ./node_modules/.bin/webpack --env=prod --progress --profile --colors --config webpack.config.production.js",
    "dev": "./node_modules/.bin/webpack-dev-server --progress --profile",
    "lint": "./node_modules/.bin/eslint src/javascript/*.js src/javascript/**/*.js src/javascript/*.jsx src/javascript/**/*.jsx tests/**/*.js tests/*.js ",
    "test": "./node_modules/karma/bin/karma start --single-run --browsers PhantomJS",
    "test:watch": "./node_modules/karma/bin/karma start",
    "start": "npm run dev"
  },
  "keywords": [
    "react",
    "webpack",
    "babel",
    "react-router",
    "webpack-2"
  ],
  "pre-commit": {
